{
  "parser": "babel-eslint",
  "env": {
    "es6": true
  },
  "extends": "standard",
  "globals": {
    "Atomics": "readable",
    "SharedArrayBuffer": "readable",
    "__DEV__": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-uses-vars": 2
  }
}
